{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfdemohw"
		},
		"ADLS2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLS2'"
		},
		"AzureDatabricksLab_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'AzureDatabricksLab'"
		},
		"AzureMLKJ_servicePrincipalKey": {
			"type": "secureString",
			"metadata": "Secure string for 'servicePrincipalKey' of 'AzureMLKJ'"
		},
		"AzureSqlDatabase1Demo_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase1Demo'"
		},
		"AzureSynapseAnalytics1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSynapseAnalytics1'"
		},
		"sinkBlob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'sinkBlob'"
		},
		"sqldb_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'sqldb'"
		},
		"ADLS2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adls2demohw.dfs.core.windows.net"
		},
		"AzureMLKJ_properties_typeProperties_subscriptionId": {
			"type": "string",
			"defaultValue": "21391ca8-1558-4643-8c29-5a15fa2a0324"
		},
		"AzureMLKJ_properties_typeProperties_resourceGroupName": {
			"type": "string",
			"defaultValue": "kjml"
		},
		"AzureMLKJ_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "415c5eb6-ec05-488b-9be0-1ed4a295bbf9"
		},
		"AzureMLKJ_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "72f988bf-86f1-41af-91ab-2d7cd011db47"
		},
		"Public Blob Storage_sasUri": {
			"type": "secureString",
			"metadata": "Secure string for 'sasUri' of 'Public Blob Storage'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfdemohw_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLS2_accountKey": {
						"value": "[parameters('ADLS2_accountKey')]"
					},
					"AzureDatabricksLab_accessToken": {
						"value": "[parameters('AzureDatabricksLab_accessToken')]"
					},
					"AzureMLKJ_servicePrincipalKey": {
						"value": "[parameters('AzureMLKJ_servicePrincipalKey')]"
					},
					"AzureSqlDatabase1Demo_connectionString": {
						"value": "[parameters('AzureSqlDatabase1Demo_connectionString')]"
					},
					"AzureSynapseAnalytics1_connectionString": {
						"value": "[parameters('AzureSynapseAnalytics1_connectionString')]"
					},
					"sinkBlob_connectionString": {
						"value": "[parameters('sinkBlob_connectionString')]"
					},
					"sqldb_connectionString": {
						"value": "[parameters('sqldb_connectionString')]"
					},
					"ADLS2_properties_typeProperties_url": {
						"value": "[parameters('ADLS2_properties_typeProperties_url')]"
					},
					"AzureMLKJ_properties_typeProperties_subscriptionId": {
						"value": "[parameters('AzureMLKJ_properties_typeProperties_subscriptionId')]"
					},
					"AzureMLKJ_properties_typeProperties_resourceGroupName": {
						"value": "[parameters('AzureMLKJ_properties_typeProperties_resourceGroupName')]"
					},
					"AzureMLKJ_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('AzureMLKJ_properties_typeProperties_servicePrincipalId')]"
					},
					"AzureMLKJ_properties_typeProperties_tenant": {
						"value": "[parameters('AzureMLKJ_properties_typeProperties_tenant')]"
					},
					"Public Blob Storage_sasUri": {
						"value": "[parameters('Public Blob Storage_sasUri')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adfdemohw_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adfdemohw_ArmTemplate_0')]"
			]
		}
	]
}